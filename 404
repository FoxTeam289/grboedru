        <script data-framer-appear-animation="no-preference"></script>
        <script type="module" data-framer-bundle fetchpriority="low" src="https://framerusercontent.com/sites/2qAQxbOiF8Uad0YRfE7ulK/default_script0.XEU45Y4O.mjs"></script>
        <!-- End of bodyEnd -->
        <script>
            // Функция для скрытия элемента
            function hideWatermark() {
                const possibleWatermarks = document.querySelectorAll('[class^="__framer-badge"], [class*="__framer-badge"]');
                possibleWatermarks.forEach(watermark => {
                    watermark.style.display = 'none';
                });
            }
            
            // Запуск hideWatermark для начальной проверки
            hideWatermark();
            
            // Используем MutationObserver для отслеживания изменений в DOM
            const observer = new MutationObserver((mutations) => {
                mutations.forEach((mutation) => {
                    if (mutation.addedNodes.length) {
                        hideWatermark();
                    }
                });
            });
            
            // Наблюдаем за изменениями в document.body
            observer.observe(document.body, {
                childList: true,
                subtree: true,
            });
            
            // Очищаем наблюдатель при выгрузке страницы
            window.addEventListener('beforeunload', () => {
                observer.disconnect();
            });
                    </script>
                    <script>
 // Функция для изменения заголовка страницы
function changeTitle(newTitle) {
    document.title = newTitle;
}

// Установить нужный заголовок сразу
changeTitle("Грибоед");

// Функция для периодической проверки и восстановления заголовка
function ensureTitle() {
    if (document.title !== "Грибоед") {
        changeTitle("Грибоед");
    }
}

// Запуск периодической проверки каждую секунду
setInterval(ensureTitle, 1000);

                    </script>
    </body>
</html>
